{
  "name": "domain-manager",
  "version": "1.0.0",
  "description": "An app to track website and domain information, such as URLs, usernames, and passwords",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "Alex Jobe",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "html": "^1.0.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1"
  },
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "nodemon": "^1.19.1"
  },
  "build": {
    "appId": "com.mai.domainmanager",
    "productName": "Domain Manager",
    "target": "NSIS",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "oneClick": false
    },
    "files": [
      "build",
      "middleware",
      "models",
      "node_modules",
      "routes",
      "package.json",
      "main.js",
      "app.js"
    ]
  }
}
